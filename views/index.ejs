<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <% include ./partials/head.ejs %>
    <title></title>
  </head>
  <body>
      <form action="/CampFlogGnaw/Saturday/MyItinerary" method="post">

        <!-- Camp Stage -->
        <div class="container">
          <div class="row">
            <div class="col-sm">
        <%for (let i=0; i < saturdayLineup.length; i++) {%>
       <% if (saturdayLineup[i].camp === true) { %>
        <h3><%=saturdayLineup[i].artist%></h3>
        <img src="<%=saturdayLineup[i].img%>" height="200px">
        <input
          type="hidden"
          name="<%=saturdayLineup[i].id%>"
          value="0"
          >
          <input
            type="checkbox"
            class="selection"
            name="<%=saturdayLineup[i].id%>"
            startTime="<%=saturdayLineup[i].trueStartTime%>"
            endTime="<%=saturdayLineup[i].trueEndTime%>"
            value="1"
            <% if (saturdayLineup[i].checked) { %>
              checked
            <% } %>
            />
          <!-- <input
            type="checkbox"
            name="half"
            value="on"
            <% if (saturdayLineup[i].half) { %>
              checked
            <% } %>
          /> -->
      <%  }%>
      <% } %>
      </div>

      <!-- Flog Stage -->
      <div class="col-sm">
        <%for (let i=0; i < saturdayLineup.length; i++) {%>
      <% if (saturdayLineup[i].flog === true) { %>
        <h3><%=saturdayLineup[i].artist%></h3>
       <img src="<%=saturdayLineup[i].img%>" height="200px">
       <input
         type="hidden"
         name="<%=saturdayLineup[i].id%>"
         value="0"
         >
         <input
           type="checkbox"
           class="selection"
           name="<%=saturdayLineup[i].id%>"
           startTime="<%=saturdayLineup[i].trueStartTime%>"
           endTime="<%=saturdayLineup[i].trueEndTime%>"
           value="1"
           <% if (saturdayLineup[i].checked) { %>
             checked
           <% } %>
           />
         <!-- <input
           type="checkbox"
           name="half"
           value="on"
           <% if (saturdayLineup[i].half) { %>
             checked
           <% } %>
         /> -->
     <%  }%>
    <%}%>
        </div>
      </div>
    </div>


      <input type="submit" name="" value="submit">

          </form>

  </body>
</html>
